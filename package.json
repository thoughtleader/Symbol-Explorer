{
  "name": "symbol-explorer",
  "version": "1.0.0",
  "description": "UTF-8 Symbol Searcher & Toolkit - Search, save, and convert symbols",
  "main": "index.html",
  "scripts": {
    "dev": "http-server -p 8080 -c-1 -o",
    "start": "http-server -p 8080",
    "serve": "http-server -p 8080 -c-1",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "netlify dev:exec drizzle-kit migrate",
    "db:studio": "netlify dev:exec drizzle-kit studio"
  },
  "keywords": [
    "symbols",
    "utf-8",
    "converter",
    "toolkit"
  ],
  "author": "boss.ux",
  "license": "MIT",
  "dependencies": {
    "@netlify/neon": "^0.1.0",
    "drizzle-orm": "^0.44.7",
    "postgres": "^3.4.0"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.5",
    "http-server": "^14.1.1"
  }
}
